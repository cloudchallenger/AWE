    <validationRules>
        <fullName>CannotAddRuleToAvailableAdvanceWorkflow</fullName>
        <active>true</active>
        <description>User will NOT be able to add additional rules to the available Workflow if Workflow  is disabled</description>
        <errorConditionFormula>ISCHANGED(Advance_Workflow__c) &amp;&amp;! Advance_Workflow__r.Enabled__c</errorConditionFormula>
        <errorMessage>New rules cannot be added to a disabled  Workflow</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Description_is_required</fullName>
        <active>true</active>
        <description>Description_is_required</description>
        <errorConditionFormula>ISBLANK(Description__c )</errorConditionFormula>
        <errorMessage>Description is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Evaluation_Rule_Can_not_be_Blank</fullName>
        <active>true</active>
        <description>Evaluation Rule Criteria is blank</description>
        <errorConditionFormula>ISBLANK(TEXT(Evaluation_Rule__c ))</errorConditionFormula>
        <errorMessage>Evaluation Rule Criteria is blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unique_Rule_Name</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (ISNEW() ||  ISCHANGED(Name)) &amp;&amp; Name =  VLOOKUP( $ObjectType.Rule__c.Fields.Name ,  $ObjectType.Rule__c.Fields.Name , Name),true,false )</errorConditionFormula>
        <errorMessage>Rule Name must be Unique</errorMessage>
    </validationRules>
