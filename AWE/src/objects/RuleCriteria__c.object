    <validationRules>
        <fullName>Cross_Formula_Required</fullName>
        <active>true</active>
        <description>CrossFormula is required if  field selected is cross formula</description>
        <errorConditionFormula>AND( CONTAINS ( Field_API_Name__c , &apos;CrossFormula&apos;), ISBLANK( CrossFormula__c ))</errorConditionFormula>
        <errorMessage>Cross formula needs to be defined.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Matching_Value_Is_Required</fullName>
        <active>true</active>
        <description>Matching Value should be a required field for all operators except the operator &quot;Between&quot;</description>
        <errorConditionFormula>AND(Not(ISBLANK(TEXT(Matching_Type__c))),Not(ISPICKVAL(Matching_Type__c , &apos;Between&apos;)),Not(ISPICKVAL(Matching_Type__c , &apos;Not Between&apos;)),Not(ISPICKVAL(Matching_Type__c , &apos;Is Null&apos;)),Not(ISPICKVAL(Matching_Type__c , &apos;Is Not Null&apos;)),Not(ISPICKVAL(Matching_Type__c , &apos;IsChanged&apos;)), ISBLANK(Matching_Value__c))</errorConditionFormula>
        <errorMessage>Please enter a matching value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Maximum_Value_Is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(Matching_Type__c , &apos;Between&apos;),ISPICKVAL(Matching_Type__c , &apos;Not Between&apos;)), ISBLANK(Maximum_Value__c))</errorConditionFormula>
        <errorDisplayField>Maximum_Value__c</errorDisplayField>
        <errorMessage>When the Operator is Between, the Maximum Value field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Minimum_Value_Is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(Matching_Type__c , &apos;Between&apos;),ISPICKVAL(Matching_Type__c , &apos;Not Between&apos;)), ISBLANK(Minimum_Value__c))</errorConditionFormula>
        <errorDisplayField>Minimum_Value__c</errorDisplayField>
        <errorMessage>When the Operator is Between, the Minimum Value field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Operator_Is_Required</fullName>
        <active>true</active>
        <description>Operator is required field</description>
        <errorConditionFormula>ISBLANK( TEXT(Matching_Type__c) )</errorConditionFormula>
        <errorMessage>Operator value is required</errorMessage>
    </validationRules>
